    </servlet>
    <servlet-mapping>
        <servlet-name>BookingServlet</servlet-name>
        <url-pattern>/booking/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MonthServlet</servlet-name>
        <servlet-class>com.wavemaker.controller.MonthServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MonthServlet</servlet-name>
        <url-pattern>/month/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SearchUserServlet</servlet-name>
        <servlet-class>com.wavemaker.controller.SearchUserId</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchUserServlet</servlet-name>
        <url-pattern>/searchUser/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>OnlyRoomServlet</servlet-name>
