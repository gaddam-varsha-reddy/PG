        <servlet-name>PaymentServlet</servlet-name>
        <url-pattern>/payment/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>VaccantRoomServlet</servlet-name>
        <servlet-class>com.wavemaker.controller.VaccantRoomServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VaccantRoomServlet</servlet-name>
        <url-pattern>/vaccantroom/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.wavemaker.controller.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/userdetails/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BedServlet</servlet-name>
